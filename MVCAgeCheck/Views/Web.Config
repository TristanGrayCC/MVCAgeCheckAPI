<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system>
    <security>
      <authentication>
        <basicauthentication enabled="false"></basicauthentication>
      </authentication>
    </security>
    <httpprotocol>
      <customheaders>
        <clear></clear>
        <add name="X-Powered-By" value="ASP.NET"></add>
      </customheaders>
    </httpprotocol>
    <directorybrowse enabled="false"></directorybrowse>
    <defaultdocument>
      <files>
        <clear></clear>
        <add value="Default.htm"></add>
        <add value="Default.asp"></add>
        <add value="index.asp"></add>
        <add value="index.htm"></add>
        <add value="index.html"></add>
        <add value="index.php"></add>
        <add value="Default.aspx"></add>
      </files>
    </defaultdocument>
    <system.web.webPages.razor>
      <host factoryType= "..." />
      <pages pageBaseType="System.Web.Mvc.WebViewPage">
        <namespaces>
          <add namespace="System.Web.Mvc" />
          <add namespace="System.Web.Mvc.Ajax" />
          <add namespace="System.Web.Mvc.Html" />
          <add namespace="System.Web.Routing" />
          <add namespace="System.Web.Optimization" />
        </namespaces>
      </pages>
    </system.web.webPages.razor>
  </system>
</configuration>
